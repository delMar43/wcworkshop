                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Dialog">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute type="xs:string" name="Commands" use="optional"/>
                            <xs:attribute type="xs:string" name="FacialExpressions" use="optional"/>
                            <xs:attribute type="xs:string" name="LipSyncText" use="optional"/>
                            <xs:attribute type="xs:byte" name="BackgroundImageIndex" use="optional"/>
                            <xs:attribute type="xs:byte" name="ForegroundImageIndex" use="optional"/>
                            <xs:attribute type="xs:byte" name="TextColorIndex" use="optional"/>
                            <xs:attribute type="xs:short" name="Delay" use="optional"/>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
