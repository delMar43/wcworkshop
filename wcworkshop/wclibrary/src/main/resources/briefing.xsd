                <xs:complexType>
                  <xs:choice maxOccurs="unbounded" minOccurs="0">
                    <xs:element name="DialogSetting">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute type="xs:byte" name="BackgroundImageIndex" use="optional"/>
                            <xs:attribute type="xs:byte" name="ForegroundImageIndex" use="optional"/>
                            <xs:attribute type="xs:byte" name="TextColorIndex" use="optional"/>
                            <xs:attribute type="xs:short" name="Delay" use="optional"/>
                          </xs:extension>
