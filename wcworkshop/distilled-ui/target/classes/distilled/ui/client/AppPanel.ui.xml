  <g:DockLayoutPanel>
    <g:north size="25">
      <g:MenuBar ui:field="menuBar">
        <g:MenuItem text="menu">
          <g:MenuBar>
            <g:MenuItem text="sub"></g:MenuItem>
          </g:MenuBar>
        </g:MenuItem>
        <g:MenuItem text="two"></g:MenuItem>
      </g:MenuBar>
    </g:north>

    <g:west size="120">
      <g:Tree height="100%" ui:field="navTree">
